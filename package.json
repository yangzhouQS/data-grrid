{
  "name": "@hodgef/ts-library-boilerplate-basic",
  "version": "1.0.155",
  "description": "Typescript Minimal Starter Kit for building libraries",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.demo.js",
    "build": "webpack && tsc",
    "build:ts": "rimraf dist-ts && tsc --project ./tsconfig.json",
    "build:dts": "dts-bundle --name cheetah-grid --main dist-ts/main.d.ts --out ../dist/index.d.ts",
    "build:webpack": "cross-env NO_UPDATE_NOTIFIER=1 webpack",
    "build:demo": "webpack --config webpack.config.demo.js",
    "prettier": "prettier --write --parser typescript src/**/*.ts",
    "eslint": "eslint . --ext .js,.ts",
    "eslint:fix": "eslint ./src --fix --ext .js,.ts",
    "coverage": "npm run test -- --coverage",
    "prepare": "npm run build",
    "trypublish": "npm publish || true"
  },
  "url": "",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.15.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "autoprefixer": "^10.3.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.0",
    "cheetah-grid-icon-svg-loader": "^1.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "cssnano": "^4.1.11",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "image-matcher": "^0.1.4",
    "material-design-icons": "^3.0.1",
    "mini-css-extract-plugin": "^2.2.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.4.1",
    "prettier-eslint-cli": "^5.0.1",
    "prettier-webpack-plugin": "^1.2.0",
    "promise-polyfills": "^7.0.4",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.13.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^8.1.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.48.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "4.0.0-rc.0"
  },
  "engines": {
    "node": ">=15"
  }
}
